[configuration]

entry_symbol = "html_library_init"
compatibility_minimum = "4.3"

[dependencies]

macos.debug = {
    "bin/macos/libUltralight.dylib" : "",
    "bin/macos/libUltralightCore.dylib" : "",
    "bin/macos/libWebCore.dylib" : "",
    "bin/macos/libAppCore.dylib" : "",
}

macos.release = {
    "bin/macos/libUltralight.dylib" : "",
    "bin/macos/libUltralightCore.dylib" : "",
    "bin/macos/libWebCore.dylib" : "",
    "bin/macos/libAppCore.dylib" : "",
}

windows.debug.x86_64 = {
    "bin/windows/Ultralight.dll" : "",
    "bin/windows/UltralightCore.dll" : "",
    "bin/windows/WebCore.dll" : "",
    "bin/windows/AppCore.dll" : "",
}

windows.release.x86_64 = {
    "bin/windows/Ultralight.dll" : "",
    "bin/windows/UltralightCore.dll" : "",
    "bin/windows/WebCore.dll" : "",
    "bin/windows/AppCore.dll" : "",
}

linux.debug.x86_64 = {
    "bin/linux/libUltralight.so" : "",
